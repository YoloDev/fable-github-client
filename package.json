{
  "name": "fable-githubclient",
  "private": true,
  "devDependencies": {
    "ava": "^0.19.1",
    "ava-check": "^1.0.0-rc.0",
    "babel-core": "^6.24.1",
    "babel-plugin-import-rename": "^1.0.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-env": "^1.5.1",
    "chalk": "^1.1.3",
    "codecov": "^2.2.0",
    "convert-source-map": "^1.5.0",
    "duplexer": "^0.1.1",
    "fable-builder": "^0.1.8",
    "fable-utils": "^1.0.0",
    "figures": "^2.0.0",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.7.0",
    "nyc": "^11.0.1",
    "sinon": "^2.3.1",
    "tap-parser": "^5.3.3",
    "through2": "^2.0.3"
  },
  "scripts": {
    "build": "fable build --instrument",
    "test": "nyc ava bin/js/test/**/*.js",
    "test:ci": "nyc ava --tap bin/js/test/**/*.js | node tapon",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coverage:show": "nyc report --reporter=text",
    "watch:fable": "fable build --watch",
    "watch:ava": "ava --watch bin/js/test/**/*.js"
  },
  "ava": {},
  "nyc": {
    "instrument": false,
    "sourceMap": false,
    "exclude": [
      "**/packages/**",
      "**/test/**"
    ],
    "reporter": []
  }
}
